<?xml version="1.0" encoding="utf-8"?>


<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="35"
    android:toDegrees="55"
    android:pivotX="30%"
    android:pivotY="120%">
    <shape android:shape="rectangle">

        <size android:height="1dp"
            android:width="1dp"/>
        <solid android:color="@color/primaryLightColor"/>
        <gradient
            android:startColor="@color/primaryLightColor"
            android:endColor="#6B9295"
            android:angle="90"
            />


    </shape>
</rotate>